    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.24.3",
        "@types/validator": "^13.15.10",
        "chalk": "^5.6.2",
        "dotenv": "^17.2.3",
        "https-proxy-agent": "^7.0.6",
        "inquirer": "9.3.7",
        "inquirer-autocomplete-prompt": "3.0.1",
        "is-interactive": "^2.0.0",
        "node-fetch": "^3.3.2",
        "ora": "^9.0.0",
        "supports-color": "10.2.2",
        "tslog": "^4.10.2",
        "validator": "^13.15.23"
    },
    "description": "Domo Query CLI - Interactive command-line tool for querying Domo data sources and dataflows",
    "devDependencies": {
        "@commitlint/cli": "^20.3.1",
        "@commitlint/config-conventional": "^20.3.1",
        "@eslint/js": "^9.39.2",
        "@eslint/json": "^0.14.0",
        "@eslint/markdown": "^7.5.1",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^12.0.2",
        "@semantic-release/npm": "^13.1.3",
        "@semantic-release/release-notes-generator": "^14.1.0",
        "@types/inquirer": "^9.0.8",
        "@types/inquirer-autocomplete-prompt": "^3",
        "@types/node": "^25.0.9",
        "@types/node-fetch": "2",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "@vitest/coverage-v8": "4.0.17",
        "@vitest/utils": "^4.0.17",
        "@yarnpkg/sdks": "^3.2.3",
        "depcheck": "^1.4.7",
        "esbuild": "^0.27.2",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-plugin-tsdoc": "^0.5.0",
        "globals": "^17.0.0",
        "husky": "^9.1.7",
        "json-schema-to-typescript": "^15.0.4",
        "openapi-typescript": "^7.10.1",
        "prettier": "^3.8.0",
        "semantic-release": "^25.0.2",
        "ts-node": "^10.9.2",
        "ts-node-register": "^1.0.0",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.0",
        "vite": "^7.3.1",
        "vite-node": "^5.3.0",
        "vitest": "^4.0.17"
    },
    "license": "GPL-3.0-only",
    "name": "domo-query-cli",
    "packageManager": "yarn@4.9.4",
    "scripts": {
        "build": "rm -rf dist && node esbuild.config.mjs && tsc --emitDeclarationOnly --outDir dist",
        "build:dist": "node build.mjs",
        "build:mcp": "yarn mcp:build",
        "build:all": "yarn build:dist && yarn build:mcp",
        "check": "yarn format && yarn lint:fix && yarn tsc --noEmit",
        "dev": "tsc --watch --noEmit",
        "format": "prettier --write .",
        "format:posttooluse": "prettier --write ",
        "format:check": "prettier --check .",
        "generate:api-types": "node scripts/generate-api-types.js",
        "generate:types": "yarn generate:api-types",
        "install:dev": "bash scripts/install-dev.sh",
        "lint": "eslint . --ext .ts,.mjs,.cjs",
        "lint:posttooluse": "eslint --fix ",
        "lint:fix": "eslint . --ext .ts,.mjs,.cjs --fix",
        "mcp:build": "rm -rf mcp/dist && node mcp/build-mcp.mjs",
        "mcp:start": "node mcp/dist/mcp-server.cjs",
        "mcp:dev": "yarn mcp:build && yarn mcp:start",
        "postinstall": "husky",
        "release": "semantic-release",
        "release:dry-run": "semantic-release --dry-run",
        "release:check": "GITHUB_TOKEN=dummy yarn dlx semantic-release --dry-run --no-ci --branches=$(git branch --show-current)",
        "release:preview": "GITHUB_TOKEN=dummy yarn dlx semantic-release --dry-run --no-ci --branches=$(git branch --show-current) --debug",
        "shell": "tsx src/main.ts",
        "start": "tsx src/main.ts",
        "test": "vitest --run",
        "test:coverage": "vitest --coverage",
        "test:watch": "vitest --watch",
        "typecheck": "tsc --noEmit",
        "typecheck:posttooluse": "tsc --noEmit ",
        "uninstall": "bash scripts/uninstall.sh",
        "prepare": "husky"
    },
    "type": "module",
    "version": "1.7.3"
}
