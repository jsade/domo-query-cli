{
    "author": "jsade",
    "homepage": "https://github.com/jsade/domo-query-cli",
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "chalk": "^5.4.1",
        "dotenv": "^16.5.0",
        "https-proxy-agent": "^7.0.6",
        "inquirer": "8.2.6",
        "inquirer-autocomplete-prompt": "2.0.1",
        "is-interactive": "^2.0.0",
        "node-fetch": "^3.3.2",
        "ora": "^8.2.0",
        "supports-color": "8.1.1",
        "tslog": "^4.9.3"
    },
    "description": "Domo Query CLI - Interactive command-line tool for querying Domo data sources and dataflows",
    "devDependencies": {
        "@eslint/js": "^9.25.1",
        "@eslint/json": "^0.12.0",
        "@eslint/markdown": "^6.4.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^10.1.3",
        "@semantic-release/npm": "^12.0.1",
        "@semantic-release/release-notes-generator": "^14.0.3",
        "@types/inquirer": "^9.0.8",
        "@types/inquirer-autocomplete-prompt": "^2",
        "@types/node": "^22.15.2",
        "@types/node-fetch": "2",
        "@typescript-eslint/eslint-plugin": "^8.31.0",
        "@typescript-eslint/parser": "^8.31.0",
        "@vitest/coverage-v8": "3.1.2",
        "@vitest/utils": "^3.2.1",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "depcheck": "^1.4.7",
        "esbuild": "^0.25.5",
        "eslint": "^9.25.1",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-prettier": "^5.2.6",
        "eslint-plugin-tsdoc": "^0.4.0",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "json-schema-to-typescript": "^15.0.4",
        "openapi-typescript": "^7.7.1",
        "prettier": "^3.5.3",
        "semantic-release": "^24.2.5",
        "ts-node": "^10.9.2",
        "ts-node-register": "^1.0.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.31.0",
        "vite": "^6.3.4",
        "vite-node": "^3.1.3",
        "vitest": "^3.1.2"
    },
    "license": "GPL-3.0-only",
    "name": "domo-query-cli",
    "packageManager": "yarn@4.9.2",
    "scripts": {
        "build": "rm -rf dist && node esbuild.config.mjs && tsc --emitDeclarationOnly --outDir dist",
        "build:dist": "node build.mjs",
        "check": "yarn format && yarn lint:fix && yarn tsc --noEmit",
        "commit": "cz",
        "dev": "tsc --watch --noEmit",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "generate:api-types": "node scripts/generate-api-types.js",
        "generate:types": "yarn generate:api-types",
        "lint": "eslint . --ext .ts,.mjs,.cjs",
        "lint:fix": "eslint . --ext .ts,.mjs,.cjs --fix",
        "postinstall": "husky",
        "release": "semantic-release",
        "release:dry-run": "semantic-release --dry-run",
        "release:check": "GITHUB_TOKEN=dummy yarn dlx semantic-release --dry-run --no-ci --branches=$(git branch --show-current)",
        "release:preview": "GITHUB_TOKEN=dummy yarn dlx semantic-release --dry-run --no-ci --branches=$(git branch --show-current) --debug",
        "shell": "tsx src/main.ts",
        "start": "tsx src/main.ts",
        "test": "vitest --run",
        "test:coverage": "vitest --coverage",
        "test:watch": "vitest --watch",
        "typecheck": "tsc --noEmit",
        "prepare": "husky"
    },
    "type": "module",
    "version": "0.0.1"
}
